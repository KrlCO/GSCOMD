<Window x:Class="GSCOMD_2._0.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GSCOMD_2._0"
        mc:Ignorable="d"
        Title="GSCOMD" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="163*"/>
            <ColumnDefinition Width="101*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="txtCodeTrab" HorizontalAlignment="Left" Height="23" Margin="144,19,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" KeyDown="txtCodeTrab_validacion"/>
        <Label Content="Codigo Trabajador:" HorizontalAlignment="Left" Margin="19,16,0,0" VerticalAlignment="Top"/>
        <Label Content="Tipo Comida:" HorizontalAlignment="Left" Margin="19,122,0,0" VerticalAlignment="Top"/>
        <ComboBox HorizontalAlignment="Left" Margin="144,122,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="ComboBox_SelectionChanged"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="144,73,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
        <Label Content="Comensal:" HorizontalAlignment="Left" Margin="19,70,0,0" VerticalAlignment="Top"/>
        <Button Content="Aceptar" HorizontalAlignment="Left" Margin="20,300,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" Grid.Column="1"/>
        <Button Content="Cancelar" HorizontalAlignment="Left" Margin="158,300,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1" Grid.Column="1"/>
        <!--<ListBox Name="listaAtencion" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="400,33,31,201" SelectionChanged="listaAtencion_SelectionChanged"/>-->

        <TextBox Name="txtCodigoTrabajador" Height="23" Width="120" TextChanged="txtCodigoTrabajador_TextChanged" Margin="144,162,225,234" />
        <Label Name="lblAsignacion" Content="Estado" HorizontalAlignment="Left" Margin="19,205,0,0" VerticalAlignment="Top" Width="266"/>
        <Image x:Name="imgTrab" Grid.Column="1" HorizontalAlignment="Left" Height="227" Margin="41,19,0,0" VerticalAlignment="Top" Width="227"/>
        <Label x:Name="lblNombTrab" Content="" HorizontalAlignment="Left" Margin="290,19,0,0" VerticalAlignment="Top" Width="179" Height="23"/>
        <!--<Label Name="lblAsignacion" Content="Estado" FontWeight="Bold" Grid.ColumnSpan="2" />-->

    </Grid>
</Window>
